    "@blueprintjs/datetime": "v3.0",
    "@blueprintjs/select": "v3.0",
    "@blueprintjs/table": "v3.0",
    "@types/react-motion": "^0.0.26",
    "apollo-cache-inmemory": "1.2.5",
    "apollo-client": "^2.3.4",
    "apollo-link": "^1.2.2",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "formik": "1.0.0-beta.2",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "import-sort-style-eslint": "^4.2.0",
    "import-sort-style-module": "^4.2.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "rc-time-picker": "^3.3.1",
    "react": "^16.4.1",
    "react-apollo": "^2.1.5",
    "react-collapse": "^4.0.3",
    "react-color": "^2.14.1",
    "react-dom": "^16.4.1",
    "react-motion": "^0.5.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.16.0",
    "react-times": "^3.1.3",
    "react-transition-group": "^2.3.1",
    "resolve": "1.8.1",
