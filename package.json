    "@types/react-router-dom": "^4.2.7",
    "@types/yup": "^0.24.6",
    "apollo": "^1.1.1",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "husky": "^1.0.0-rc.13",
    "prettier": "^1.13.7",
