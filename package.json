{
  "name": "timephile-frontend",
  "version": "0.1.2",
  "private": true,
  "main": "/src/App.tsx",
  "engines": {
    "node": "10.4.1"
  },
  "dependencies": {
    "@blueprintjs/core": "^2.3.1",
    "@blueprintjs/datetime": "^2.0.3",
    "@blueprintjs/select": "^2.0.1",
    "@blueprintjs/table": "^2.1.1",
    "apollo-cache-inmemory": "^1.2.4",
    "apollo-client": "^2.3.4",
    "apollo-link": "^1.2.2",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "formik": "1.0.0-beta.2",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "import-sort-style-eslint": "^4.2.0",
    "import-sort-style-module": "^4.2.0",
    "moment": "^2.22.2",
    "rc-time-picker": "^3.3.1",
    "react": "^16.4.1",
    "react-apollo": "^2.1.5",
    "react-color": "^2.14.1",
    "react-dom": "^16.4.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.16.0",
    "react-times": "^3.1.3",
    "react-transition-group": "^2.3.1",
    "yup": "^0.25.1"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "codegen:generate": "apollo codegen:generate --schema=./schema.json --queries=./src/**/*.ts --target=typescript ./src/schemaTypes.ts",
    "schema:download": "apollo schema:download --endpoint=http://localhost:4000",
    "gen-types": "yarn run schema:download && yarn run codegen:generate"
  },
  "devDependencies": {
    "@types/jest": "^23.1.0",
    "@types/node": "^10.3.3",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.3.18",
    "@types/react-color": "^2.13.5",
    "@types/react-dom": "^16.0.6",
    "@types/react-router": "^4.0.26",
    "@types/react-router-dom": "^4.2.7",
    "@types/yup": "^0.24.6",
    "apollo": "^1.1.1",
    "import-sort-cli": "^4.2.0",
    "import-sort-parser-babylon": "^4.2.0",
    "import-sort-parser-typescript": "^4.2.0",
    "import-sort-style-renke": "^4.2.0",
    "typescript": "^2.9.2"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint"
    }
  }
}
